{
  "name": "carefinder",
  "version": "1.0.0",
  "description": "Lambda function for Carefinder Alexa skill",
  "main": "index.js",
  "scripts": {
      "invoke": "aws lambda invoke --function-name 'CareFinderAlexaSkill' --payload fileb://../tests/events/closest-er-intent.json ../tests/out.txt && cat ../tests/out.txt",
      "test": "echo \"Error: no test specified\" && exit 1",
      "deploy": "npm run zip && npm run upload",
      "upload": "aws lambda update-function-code --function-name 'CareFinderAlexaSkill' --zip-file fileb://../index.zip",
      "zip": "zip -r ../index.zip . -x 'package.json' -x '*.git*' "
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/web-q/carefinder.git"
  },
  "keywords": [
    "alexa",
    "skill",
    "node.js"
  ],
  "author": "Albert Koncz",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/web-q/carefinder/issues"
  },
  "homepage": "https://github.com/web-q/carefinder#readme",
  "dependencies": {
    "alexa-sdk": "^1.0.9"
  }
}
